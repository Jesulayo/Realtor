    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:prism="http://prismlibrary.com"
    prism:ViewModelLocator.AutowireViewModel="True">

    <StackLayout>
        <Label Text="{Binding Title}" />
        <Image Source="{Binding Image}" />
    </StackLayout>

</ContentPage>